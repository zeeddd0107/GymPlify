{
  "name": "gymplify",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "prepare": "husky install",
    "test": "npm run test:backend && npm run test:web && npm run test:mobile",
    "test:backend": "npm --prefix backend test",
    "test:web": "npm --prefix web test || echo '⚠️  No tests in web yet'",
    "test:mobile": "npm --prefix mobile test || echo '⚠️  No tests in mobile yet'",
    "lint": "eslint .",
    "lint:all": "node lint-all.cjs"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.28.0",
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-react": "^7.27.1",
    "@eslint/js": "^9.29.0",
    "babel-jest": "^30.0.0",
    "eslint": "^9.29.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.2.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "prettier": "^3.5.3"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@expo/webpack-config": "~19.0.1",
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@react-native-async-storage/async-storage": "1.21.0",
    "expo-auth-session": "~5.4.0",
    "expo-crypto": "~12.8.1",
    "expo-web-browser": "~12.8.2",
    "react-dom": "18.2.0",
    "react-native-qrcode-svg": "^6.3.15",
    "react-router-dom": "^7.6.3"
  }
}
